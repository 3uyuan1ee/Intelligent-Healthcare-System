}

#pushButton_2:hover {
    background-color: #8b5cf6;
    color: white;
}

#pushButton_2:pressed {
    background-color: #7c3aed;
    border-color: #7c3aed;
}

/* 图片样式 */
QLabel[scaledContents=&quot;true&quot;] {
    border-radius: 8px;
}
